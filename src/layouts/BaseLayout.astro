---
import Header from "../components/Header.astro"
import Footer from "../components/Footer.astro"

const { pageTitle } = Astro.props;
---


<!DOCTYPE html>
<html lang="ja">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{pageTitle}</title>
</head>
<body>
	<Header />

    <main>
			<div class="">
				Home
			</div>
      <slot />
      <div class="content">
        <section class="posts">
        </section>
      </div>
    </main>

	<Footer />
</body>
</html>
